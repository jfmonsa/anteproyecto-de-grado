{
  "files.exclude": {
    "**/*.aux": true,
    "**/*.log": true,
    "**/*.out": true,
    "**/*.toc": true,
    "**/*.bbl": true,
    "**/*.blg": true,
    "**/*.lof": true,
    "**/*.lot": true,
    "**/*.fls": true,
    "**/*.fdb_latexmk": true,
    "**/*.synctex.gz": true,
    "**/*.dvi": true,
    "**/*.run.xml": true,
    "**/*.bcf": true
  },
  "latex-workshop.formatting.latex": "tex-fmt",
  "editor.formatOnSave": true,
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop",
    "editor.wordWrap": "on"
  },
  "cSpell.words": [
    "Bertalanffy",
    "biber",
    "biblatex",
    "Chavarriaga",
    "chktex",
    "Colciencias",
    "Esponda",
    "Fargate",
    "instalela",
    "Kansara",
    "kean",
    "Kyadasu",
    "legaltech",
    "Legaltech",
    "libxcrypt",
    "mantenibilidad",
    "Opara",
    "pacman",
    "Paralegales",
    "polyfiling",
    "powerstat",
    "provisionados",
    "Royer",
    "Sansui",
    "tlmgr",
    "upme",
    "Webcloster"
  ],
  "cSpell.ignorePaths": [
    "bibliografia.bib",
    "settings.json",
    "extensions.json",
    "scripts/*",
    "latex-packages.txt",
    ".gitignore",
    "Taskfile.yml",
    ".github/*"
  ]
}
